WINDOW 1,"SpaceDepugger",(0,15)-(640,480),4FOR i=1 TO 2MENU i,0,0,""NEXTIntro:RANDOMIZE TIMERbackcolor 33CLSpi=3.14159:aste=1FOR k=1 TO 360 STEP 5forecolor 409D=k*pi/180CIRCLE (250,160),aste,,0,Daste=aste+1NEXTFOR u=1 TO 4000:NEXT uforecolor 205PRINT:PRINTCALL TEXTFONT(3):CALL TEXTSIZE (33)PRINT TAB(6)" SpaceDebugger"LOCATE 8,8CALL TEXTSIZE (14):CALL TEXTMODE (1)forecolor 341PRINT "Made by Johannes Tuikkala"LOCATE 22,20forecolor 137CALL TEXTSIZE (12)PRINT "Click mouse to continue"WHILE MOUSE(0)<>-1: WENDAlustus:DIM olio%(202)DIM ref%,valikko!,damage!,alus!,voitto!,main!,help!,prec%(44),pprec%(44),cprec(44)DIM eprec(3),mrec%(3),rprec%(3)ref%=SYSTEM(7)voitto%=81:dam%=27:x=1:b%=400:df=0:pi=3.14159:aste=1:taso%=0:C=1:pist=0forecolor 69backcolor 33CLSRANDOMIZE TIMERsetRect prec%(0),14,14,350,145GetPicture ref%,   128     ,valikko!FOR k=1 TO 360 STEP 5D=k*pi/180CIRCLE (250,160),aste,,0,Daste=aste+.1NEXT kGET (234,144)-(268,178),olio%CLSA%=200PRINTPRINT'CALL TEXTFONT (12)CALL TEXTSIZE(13)setRect mrec%(0),10,140,350,145GetPicture ref%,   1     ,main!mainscreen:DrawPicture main,,mrec%(0)w$=INKEY$IF w$="" THEN mainscreenIF (w$="q" OR w$="Q") THEN ENDIF (w$="p" OR w$="P") THEN alIF (w$="h" OR w$="H") THENCLSsetRect rprec%(0),40,10,0,0GetPicture ref%,   2     ,help!DrawPicture help!,,rprec%(0)WHILE MOUSE(0)<>-1:WENDCLSEND IFGOTO mainscreenal:CLSDrawPicture valikko,,prec%(0)forecolor 33LINE (0,89)-(640,89)aloitus:CALL TEXTMODE(8)CALL TEXTSIZE(11)LOCATE 4,66 PRINT pistRANDOMIZE TIMERIF df=31 THENCLS'CALL TEXTFONT (3)CALL TEXTSIZE (50)PRINTPRINTforecolor 205PRINT "GOOD!"WAVE 3,SINC=523D=587E=659F=698G=784A=880H=988T=0SOUND G,2,,3SOUND F,2,,3SOUND E,4,,3SOUND E,4,,3SOUND T,4,,3SOUND E,4,,3SOUND F,4,,3SOUND F,4,,3SOUND T,4,,3SOUND F,2,,3SOUND E,2,,3SOUND D,4,,3SOUND D,4,,3SOUND T,4,,3SOUND D,4,,3SOUND E,8,,3SOUND T,4,,3SOUND G,3,,3SOUND F,3,,3SOUND E,8,,3SOUND E,4,,3SOUND E,4,,3SOUND F,12,,3SOUND G,4,,3SOUND A,4,,3SOUND F,4,,3SOUND E,8,,3SOUND D,12,,3SOUND C,4,,3SOUND C,4,,3WHILE MOUSE(0)<>-1:WENDCLSGOTO mainscreenEND IFC=C+.12IF dam%=>190 THEN ex2GOTO mainscreenEND IFarvovarix=1y%=INT(RND*400)fu=0IF y%<150 THEN y%=150FOR i%=1 TO 680 STEP CA$=INKEY$IF (A$="q" OR A$="Q") THENpist=0GOTO mainscreenEND IFIF (A$="P" OR A$="p") THENSOUND 2000,1,,3pauseEND IFIF A$="8" THEN A%=A%-6IF A$="2" THEN A%=A%+6IF A%<100 THEN A%=100IF A%>390 THEN A%=390setRect cprec%(0),380,A%,350,145GetPicture ref%,   129     ,alus!DrawPicture alus!,,cprec%(0)PUT (i%,y%),olio%,PSETcx=0IF  (y%=A%+12 OR y%=A%+13 OR y%=A%+14 OR y%=A%+15 OR y%=A%+16 OR y%=A%+17  OR y%=A%+18 OR y%=A%+19 OR y%=A%+20 OR y%=A%+21 OR y%=A%+22 OR y%=A%+23 OR y%=A%+24) THEN cx=5IF (i%= 393 OR i%=392 OR i%=391 OR i%=390 OR i%=394 OR i%=389 OR i%=388) THENIF cx<>5 THEN hyppySOUND 1000,1,,3SOUND 2000,1,,3SOUND 1000,1,,3pist=pist+5df=df+1voitto%=voitto%-2setRect eprec%(0),292,voitto%,350,145GetPicture ref%,   132     ,voitto!DrawPicture voitto!,,eprec%(0)GOTO aloitusEND IFhyppy:IF i%=>670 THENpist=pist-1SOUND 50,.2,50,3SOUND 60,.2,60,3SOUND 70,.2,90,3SOUND 100,.2,100,3SOUND 110,.2,150,3dam%=dam%+7setRect pprec%(0),dam%,40   ,104,130GetPicture ref%,   131     ,damage!DrawPicture damage!,,pprec%(0)END IFNEXT i%GOTO aloitusSUB pause STATICloop:C$=INKEY$IF C$="" THEN loopEND SUBSUB ex2 STATICSOUND 10,1,50,3SOUND 30,1,60,3SOUND 40,1,70,3SOUND 50,1,80,3SOUND 60,1,90,3SOUND 80,1,100,3SOUND 100,1,120,3SOUND 120,1,150,3SOUND 100,1,120,3SOUND 90,1,90,3SOUND 80,1,80,3SOUND 50,1,70,3SOUND 40,1,50,3SOUND 30,1,30,3SOUND 20,1,20,3SOUND 10,1,10,3backcolor 30CLSbackcolor 33CLSCALL TEXTSIZE(30)PRINT "You Lose"WHILE MOUSE(0)<>-1:WENDCLSEND SUBSUB arvovari STATICxx=INT(RND*6)IF xx=0 THEN vari=69IF xx=1 THEN vari=205IF xx=2 THEN vari=341IF xx=3 THEN vari=409IF xx=4 THEN vari=137IF xx=5 THEN vari=273IF xx=6 THEN vari=33forecolor variEND SUB